				float4 Emission_OUT145 = ( ( ( tex2D( _EmissionMask, texCoord143 ) * ( ( lerpResult283 + lerpResult285 ) + lerpResult291 ) ) * saturate( ( ( ( ( ( step( temp_output_3_0_g22 , 0.2 ) - step( temp_output_3_0_g22 , temp_output_8_0_g22 ) ) * ( step( temp_output_10_0_g22 , 0.2 ) - step( temp_output_10_0_g22 , temp_output_8_0_g22 ) ) ) + ( ( step( temp_output_3_0_g21 , 0.2 ) - step( temp_output_3_0_g21 , temp_output_8_0_g21 ) ) * ( step( temp_output_10_0_g21 , 0.2 ) - step( temp_output_10_0_g21 , temp_output_8_0_g21 ) ) ) ) + ( ( step( temp_output_3_0_g24 , 0.2 ) - step( temp_output_3_0_g24 , temp_output_8_0_g24 ) ) * ( step( temp_output_10_0_g24 , 0.2 ) - step( temp_output_10_0_g24 , temp_output_8_0_g24 ) ) ) ) + ( ( step( temp_output_3_0_g25 , 0.2 ) - step( temp_output_3_0_g25 , temp_output_8_0_g25 ) ) * ( step( temp_output_10_0_g25 , 0.2 ) - step( temp_output_10_0_g25 , temp_output_8_0_g25 ) ) ) ) ) ) * ( _EmissionMultiplier * 100.0 ) );
				
